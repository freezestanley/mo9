{
  "name": "fragment-vue-child",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host localhost --port 8082",
    "build": "vue-cli-service build",
    "rebuild": "vue-cli-service build --mode mft",
    "lint": "vue-cli-service lint",
    "app": "nodemon fragments.js",
    "build:app": "yarn run rebuild && yarn run app",
    "chaoxi": "yarn && yarn run build:app"
  },
  "dependencies": {
    "chaoxi": "^1.0.56",
    "core-js": "^2.6.9",
    "cross-env": "^6.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "koa": "^2.8.2",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.1",
    "koa2-cors": "^2.0.6",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "postcss-import": "^12.0.1",
    "postcss-normalize": "^8.0.1",
    "postcss-selector-namespace": "^3.0.1",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
